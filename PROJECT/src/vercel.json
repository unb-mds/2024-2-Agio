{
  "version": 2,
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "src"
    },
    "cmd": "python manage_scripts/create_superuser.py && gunicorn mysite.wsgi"
  }
}
